APP_NAME=Laravel
APP_ENV=local
APP_KEY="Your_AppKey"
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# ✅ MySQL Database Configuration
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=fiji_directory
DB_USERNAME=root
DB_PASSWORD=

# ✅ Sessions (change from 'database' to 'file' to avoid sessions table)
SESSION_DRIVER=file
SESSION_LIFETIME=120

# ✅ Cache (change from 'database' to 'file' to avoid cache tables)
CACHE_STORE=file
# CACHE_PREFIX=laravel_cache

# ✅ Queue (change from 'database' to 'sync' to avoid jobs table)
QUEUE_CONNECTION=sync

# ✅ Others
BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local

# ✅ Redis (optional, ignored unless explicitly used)
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# ✅ Mail Settings (logging only in dev)
MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# ✅ AWS (not used unless configured)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# ✅ Frontend (Vite)
VITE_APP_NAME="${APP_NAME}"
